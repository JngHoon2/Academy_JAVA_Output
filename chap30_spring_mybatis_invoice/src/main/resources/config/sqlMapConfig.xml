<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  
<!-- MyBatis 설정 파일로 사용, Mapper의 ResultType에서 사용할 alias 설정-->
<configuration>

	<settings> 
		<!-- 테이블의 컬럼명(role_id)과 Vo의 멤버변수(roleId) 매칭해주는 역할 -->
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>

	<typeAliases>
		<typeAlias type="com.kunil.security.vo.Employee" alias="Employee" />
		<typeAlias type="com.kunil.security.dto.EmployeeCommonDto" alias="EmployeeCommonDto" />
		<typeAlias type="com.kunil.security.vo.CustomUser" alias="CustomUser" />
		<typeAlias type="com.kunil.security.vo.AddressVo" alias="AddressVo" />
		<typeAlias type="com.kunil.security.vo.Users" alias="Users" />
		<typeAlias type="com.kunil.security.vo.Role" alias="Role" />
		<typeAlias type="com.kunil.security.vo.UserRole" alias="UserRole" />
		<typeAlias type="com.kunil.security.vo.Criteria" alias="Criteria" />
		
	</typeAliases>
		
</configuration>